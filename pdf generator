const PDFDocument = require('pdfkit');
const fs = require('fs');

const generateQuotePDF = (quoteData) => {
    const doc = new PDFDocument();
    doc.pipe(fs.createWriteStream('quote.pdf'));

    doc.fontSize(20).text('Quote Details');
    doc.fontSize(12).text(`Total Amount: ${quoteData.TotalAmount}`);

    doc.end();
};

// Call the function with the quote data to generate the PDF
generateQuotePDF(quoteData);
